{
  "name": "vscode-fish-text",
  "publisher": "mamoru",
	"displayName": "Рыбный Текст",
  "description": "Adds russian \"fish\" text",
  "publisher": "mamoruuu",
  "galleryBanner": {
    "color": "#c42b44"
  },
  "author": {
    "name": "mamoruuu",
    "url": "https://mamoruuuu.github.io"
  },
  "repository": {
		"type": "git",
		"url": "https://github.com/mamoruuu/vscode-fish-text.git"
	},
  "icon": ".github/carpe.png",
  "version": "1.0.0",
  "license": "MIT",
	"engines": {
		"vscode": "^1.18.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "fishtext.addParagraph",
				"title": "РыбаТекст: Добавить абзац текста"
			}
    ],
    "snippets": [
      {
        "languages": [
          "html",
          "javascriptreact",
          "typescriptreact",
          "handlebars",
          "plaintext",
          "php",
          "markdown",
          "jade"
        ]
      }
    ]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "npm run compile && node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^3.1.4",
		"vscode": "^1.1.25",
		"tslint": "^5.8.0",
		"@types/node": "^8.10.25",
		"@types/mocha": "^2.2.42"
	},
	"dependencies": {
		"speech-code": "1.0.3"
	}
}
