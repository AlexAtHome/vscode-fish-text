{
  "name": "vscode-fish-text",
  "publisher": "mamoru",
  "displayName": "Рыбный Текст",
  "description": "Adds substitutional text in Russian",
  "galleryBanner": {
    "color": "#f4dfc2",
    "theme": "dark"
  },
  "author": {
    "name": "mamoruuu",
    "url": "https://mamoruuuu.github.io"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mamoruuu/vscode-fish-text.git"
  },
  "icon": ".github/carpe.png",
  "version": "1.0.2",
  "license": "MIT",
  "engines": {
    "vscode": "^1.18.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "fishtext.addParagraph",
        "title": "Рыбный Текст: Добавить абзац текста"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "npm run compile && node ./node_modules/vscode/bin/test"
  },
  "devDependencies": {
    "typescript": "^3.1.4",
    "vscode": "^1.1.25",
    "tslint": "^5.8.0",
    "@types/node": "^8.10.25",
    "@types/mocha": "^2.2.42"
  },
  "dependencies": {
    "speech-code": "1.3.1"
  }
}
